{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-semistandard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [true, 120],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
